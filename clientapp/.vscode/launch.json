      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:4200/index.html",
      "preLaunchTask": "npm: start",
      "pathMappings": [
        {
          "url": "webpack:///src",
          "path": "${workspaceFolder}/src"
        },
        {
          "url": "http://localhost:4200/",
          "path": "${workspaceFolder}"
        }
      ]
    }
  ]
}
